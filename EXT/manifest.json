{
  "manifest_version": 3,
  "content_security_policy": {
    "extension_page": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "name": "dictionary",
  "version": "0.2",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "/src/app.js"
      ],
      "css": []
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "declarativeContent"
  ]
}